{
    "name": "jey/lumen-helpers",
    "description": "To support Laravel 's unsupported features in Lumen.[Forked from albertcht/lumen-helpers]",
    "keywords": ["heplers", "lumen", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javad Sadeghi",
            "email": "sadeghij87@gmail.com"
        }
    ],
    "require": {
        "php": "^8.*",
        "laravel/lumen-framework": "^9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AlbertCht\\Lumen\\Helpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
